@model IEnumerable<Data.ViewModels.InternationalLandlinePhoneCall>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>International Calls Expenses</h1>

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Extension)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Expense)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Percent)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Extension)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Expense)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Percent)
            </td>
            @* <td> *@
            @*     <a asp-action="Edit" asp-route-id="@item.PhoneNumber">Edit</a> | *@
            @*     <a asp-action="Details" asp-route-id="@item.PhoneNumber">Details</a> | *@
            @*     <a asp-action="Delete" asp-route-id="@item.PhoneNumber">Delete</a> *@
            @* </td> *@
        </tr>
    }
    </tbody>
</table>